+std::vector<std::string> NWebApplication::CustomSchemeCmdLineSplit(std::string str, const char split)
+{
+  std::istringstream inStream(str);
+  std::vector<std::string> ret;
+  std::string token;
+  while (getline(inStream, token, split)) {
+    if (!token.empty()) {
+      ret.push_back(token);
+      token.clear();
+    }
+  }
+  return ret;
+}
