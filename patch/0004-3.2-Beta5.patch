+-TfLiteRegistration* Register_tftext_WhitespaceTokenizer() {
+-  static TfLiteRegistration r = {nullptr, nullptr,
+-                                 whitespace_tokenizer::Prepare,
+-                                 whitespace_tokenizer::Eval};
+-  return &r;
+-}
